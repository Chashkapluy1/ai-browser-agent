[flake8]
max-line-length = 100
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .eggs,
    .tox,
    .pytest_cache,
    .mypy_cache,
    .coverage,
    htmlcov,
    .idea,
    .vscode,
    *.pyc,
    *.pyo,
    *.pyd,
    .Python
ignore = E501,W503,E203
max-complexity = 15

